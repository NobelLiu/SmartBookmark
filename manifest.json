{
    "manifest_version": 2,
    "name": "SmartBookmark",
    "version": "1.0",
    "description": "A smart bookmark can auto update.",
    "browser_action": {
        "default_icon": "image/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "bookmarks",
        "storage"
    ]
}